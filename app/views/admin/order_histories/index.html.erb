<%= render 'layouts/c_header' %>
<div class= "container">
	<h1>注文一覧画面</h1>
	<table>
		<tr>
			<th>購入日時</th>
			<th>購入者</th>
			<th>注文個数</th>
			<th>ステータス</th>
		</tr>
		<%= @order_histories.each do |order_historie| %>
		<tr>
			<td><%= order_historie.created_at %></td>
			<td><%= order_historie.name %></td>
			<td><%=aaaaa %></td>
			<td><%= order_historie.order_status %></td>
		</tr>
		<% end %>
	</table>
</div>
