<%= render 'layouts/a_header' %>
<div class= "container">
	<h1>注文一覧画面</h1>
	<table>
		<tr>
			<th>購入日時</th>
			<th>購入者</th>
			<th>注文個数</th>
			<th>ステータス</th>
		</tr>
		<%= @order_histories.each do |order_history| %>
		<tr>
			<%= link_to admin_order_history_path(order_history.id) do %>
			<td><%= order_history.created_at %></td>
			<% end %>
			<td><%= order_history.name %></td>
			<td><%=aaaaa %></td>
			<td><%= order_history.order_status %></td>
		</tr>
		<% end %>
	</table>
</div>
