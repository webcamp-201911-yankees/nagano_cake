  <h1>商品一覧画面</h1>
    <table class="table table-hover">
      <tbody>
        <tr>
          <th>商品</th>
          <th>商品名</th>
          <th>税抜価格</th>
          <!-- <th>ジャンル</th> -->
          <!-- <th>ステータス</th> -->
        </tr>
            <% @products.each do |product| %>
        <tr>
          <td><%=product.id%></td>
          <td><%=product.product_name%></td>
          <td><%=product.tax_excluded%></td>
          <!-- <td>product.categories_name</td> -->
          <!-- <td>product.tax_excluded</td> -->
        </tr>
            <% end %>
      </tbody>
    </table>